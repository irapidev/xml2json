{
  "name": "@rapidevelop/xml2json",
  "version": "1.0.2",
  "description": "XML2JSON is a library to convert a XML file or XML text string to JSON.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "git:commit": "git-cz",
    "npm:version:major": "npm version major --no-git-tag-version",
    "npm:version:minor": "npm version minor --no-git-tag-version",
    "npm:version:patch": "npm version patch --no-git-tag-version",
    "prebuild:docs": "rm -rf ./docs/*",
    "build:docs": "jsdoc -c ./jsdoc.json",
    "prebuild": "rm -rf ./dist/*",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidevelop/xml2json.git"
  },
  "keywords": [
    "xml2json",
    "xml2js",
    "parser"
  ],
  "author": "Iman Vafadarnezhad",
  "license": "MIT",
  "private": false,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/rapidevelop/xml2json/issues"
  },
  "homepage": "https://github.com/rapidevelop/xml2json#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jsdoc": "^3.5.5"
  },
  "dependencies": {
    "request": "~2.88.0",
    "sax": "~1.2.4"
  }
}
